<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="432.0" prefWidth="370.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="academiccalendar.ui.listterms.ListTermsController">
    <stylesheets>
        <URL value="@/academiccalendar/ui/addevent/addEvent.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="432.0" prefWidth="370.0">
         <children>
            <HBox prefHeight="35.0" prefWidth="340.0" style="-fx-background-color: whitesmoke;">
               <children>
                  <Label fx:id="topLabel" prefHeight="25.0" prefWidth="419.0" text="Term Manager">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </HBox.margin>
                     <graphic>
                        <ImageView fitHeight="18.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/drag_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Label>
                  <Label onMouseClicked="#exit" prefHeight="37.0" prefWidth="39.0" stylesheets="@../addevent/addEvent.css">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/exit_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Label>
               </children>
            </HBox>
            <Label prefHeight="34.0" prefWidth="471.0" text="Your Terms:">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="tableView" prefHeight="287.0" prefWidth="350.0">
               <columns>
                  <TableColumn fx:id="termCol" prefWidth="170.0" text="Term" />
                  <TableColumn fx:id="dateCol" prefWidth="179.0" text="Starting Date" />
               </columns>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TableView>
            <HBox alignment="CENTER" prefHeight="31.0" prefWidth="459.0">
               <children>
                  <JFXButton alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#editTerm" prefHeight="32.0" prefWidth="130.0" text="Edit Term" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="376.0" prefHeight="31.0" prefWidth="459.0">
               <children>
                  <JFXButton alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#editTerm" prefHeight="32.0" prefWidth="145.0" text="Delete Term">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </JFXButton>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
